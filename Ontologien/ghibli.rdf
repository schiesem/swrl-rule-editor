                                  <rdf:rest>
                                    <swrl:AtomList>
                                      <rdf:first>
                                        <swrl:IndividualPropertyAtom>
                                          <swrl:propertyPredicate rdf:resource="#hasAssignment"/>
                                          <swrl:argument1 rdf:resource="#CS"/>
                                          <swrl:argument2 rdf:resource="#T"/>
                                        </swrl:IndividualPropertyAtom>
                                      </rdf:first>
                                      <rdf:rest>
                                        <swrl:AtomList>
                                          <rdf:first>
                                            <swrl:IndividualPropertyAtom>
                                              <swrl:propertyPredicate rdf:resource="#hasAssignment"/>
                                              <swrl:argument1 rdf:resource="#CS"/>
                                              <swrl:argument2 rdf:resource="#S"/>
                                            </swrl:IndividualPropertyAtom>
                                          </rdf:first>
                                          <rdf:rest>
                                            <swrl:AtomList>
                                              <rdf:first>
                                                <swrl:IndividualPropertyAtom>
                                                  <swrl:propertyPredicate rdf:resource="#hasAssignment"/>
                                                  <swrl:argument1 rdf:resource="#CS"/>
                                                  <swrl:argument2 rdf:resource="#P1"/>
                                                </swrl:IndividualPropertyAtom>
                                              </rdf:first>
                                              <rdf:rest>
                                                <swrl:AtomList>
                                                  <rdf:first>
                                                    <swrl:IndividualPropertyAtom>
                                                      <swrl:propertyPredicate rdf:resource="#hasAssignment"/>
                                                      <swrl:argument1 rdf:resource="#CS"/>
                                                      <swrl:argument2 rdf:resource="#P2"/>
                                                    </swrl:IndividualPropertyAtom>
                                                  </rdf:first>
                                                  <rdf:rest>
                                                    <swrl:AtomList>
                                                      <rdf:first>
                                                        <swrl:IndividualPropertyAtom>
