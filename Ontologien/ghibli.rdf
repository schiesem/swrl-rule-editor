                                  <swrl:argument2 rdf:resource="#P2"/>
                                </swrl:DifferentIndividualsAtom>
                              </rdf:first>
                              <rdf:rest>
                                <swrl:AtomList>
                                  <rdf:first>
                                    <swrl:ClassAtom>
                                      <swrl:classPredicate rdf:resource="#Inference"/>
                                      <swrl:argument1 rdf:resource="#I"/>
