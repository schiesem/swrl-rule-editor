<swrl:Imp>
  <swrl:body>
    <swrl:AtomList>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#SystemComponent"/>
          <swrl:argument1 rdf:resource="#A"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <swrl:AtomList>
          <rdf:first>
            <swrl:ClassAtom>
              <swrl:classPredicate rdf:resource="#Communication"/>
              <swrl:argument1 rdf:resource="#C"/>
            </swrl:ClassAtom>
          </rdf:first>
          <rdf:rest>
            <swrl:AtomList>
              <rdf:first>
                <swrl:DifferentIndividualsAtom>
                  <swrl:argument1 rdf:resource="#A"/>
                  <swrl:argument2 rdf:resource="#B"/>
                </swrl:DifferentIndividualsAtom>
              </rdf:first>
              <rdf:rest>
                <swrl:AtomList>
                  <rdf:first>
                    <swrl:IndividualPropertyAtom>
                      <swrl:propertyPredicate rdf:resource="#hasCommunication"/>
                      <swrl:argument1 rdf:resource="#B"/>
                      <swrl:argument2 rdf:resource="#C"/>
                    </swrl:IndividualPropertyAtom>
                  </rdf:first>
                  <rdf:rest>
                    <swrl:AtomList>
                      <rdf:first>
                        <swrl:IndividualPropertyAtom>
                          <swrl:propertyPredicate rdf:resource="#hasCommunication"/>
                          <swrl:argument1 rdf:resource="#A"/>
                          <swrl:argument2 rdf:resource="#C"/>
                        </swrl:IndividualPropertyAtom>
                      </rdf:first>
                      <rdf:rest>
                        <swrl:AtomList>
                          <rdf:first>
                            <swrl:ClassAtom>
                              <swrl:classPredicate rdf:resource="#TechnicalResource"/>
                              <swrl:argument1 rdf:resource="#B"/>
                            </swrl:ClassAtom>
                          </rdf:first>
                          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                        </swrl:AtomList>
                      </rdf:rest>
                    </swrl:AtomList>
                  </rdf:rest>
                </swrl:AtomList>
              </rdf:rest>
            </swrl:AtomList>
          </rdf:rest>
        </swrl:AtomList>
      </rdf:rest>
    </swrl:AtomList>
  </swrl:body>
  <swrl:head>
    <swrl:AtomList>
      <rdf:first>
        <swrl:IndividualPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#communicatesWith"/>
          <swrl:argument1 rdf:resource="#A"/>
          <swrl:argument2 rdf:resource="#B"/>
        </swrl:IndividualPropertyAtom>
      </rdf:first>
      <rdf:rest>
        <swrl:AtomList>
          <rdf:first>
            <swrl:IndividualPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#communicatesWith"/>
              <swrl:argument1 rdf:resource="#B"/>
              <swrl:argument2 rdf:resource="#A"/>
            </swrl:IndividualPropertyAtom>
          </rdf:first>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </swrl:AtomList>
      </rdf:rest>
    </swrl:AtomList>
  </swrl:head>
  <swrl2:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrl2:isRuleEnabled>
  <rdfs:comment>Es wird eine Direkte Beziehung abgeleitet wenn: A mit B und B mit C in Beziehung steht -&gt; A mit C Beziehung</rdfs:comment>
  <rdfs:label>Direkte Beziehung ableiten Communication</rdfs:label>
</swrl:Imp>

<swrl:Imp>
  <swrl:body>
    <swrl:AtomList>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#Product"/>
          <swrl:argument1 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#A"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <swrl:AtomList>
          <rdf:first>
            <swrl:ClassAtom>
              <swrl:classPredicate rdf:resource="#ProductFlow"/>
              <swrl:argument1 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#B"/>
            </swrl:ClassAtom>
          </rdf:first>
          <rdf:rest>
            <swrl:AtomList>
              <rdf:first>
                <swrl:IndividualPropertyAtom>
                  <swrl:propertyPredicate rdf:resource="#isFlowing"/>
                  <swrl:argument1 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#B"/>
                  <swrl:argument2 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#A"/>
                </swrl:IndividualPropertyAtom>
              </rdf:first>
              <rdf:rest>
                <swrl:AtomList>
                  <rdf:first>
                    <swrl:ClassAtom>
                      <swrl:classPredicate rdf:resource="#Transform"/>
                      <swrl:argument1 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#C"/>
                    </swrl:ClassAtom>
                  </rdf:first>
                  <rdf:rest>
                    <swrl:AtomList>
                      <rdf:first>
                        <swrl:IndividualPropertyAtom>
                          <swrl:propertyPredicate rdf:resource="#isTransformedBy"/>
                          <swrl:argument1 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#B"/>
                          <swrl:argument2 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#C"/>
                        </swrl:IndividualPropertyAtom>
                      </rdf:first>
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </swrl:AtomList>
                  </rdf:rest>
                </swrl:AtomList>
              </rdf:rest>
            </swrl:AtomList>
          </rdf:rest>
        </swrl:AtomList>
      </rdf:rest>
    </swrl:AtomList>
  </swrl:body>
  <swrl:head>
    <swrl:AtomList>
      <rdf:first>
        <swrl:IndividualPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#isBeingProcessedWith"/>
          <swrl:argument1 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#A"/>
          <swrl:argument2 rdf:resource="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#C"/>
        </swrl:IndividualPropertyAtom>
      </rdf:first>
      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
    </swrl:AtomList>
  </swrl:head>
  <swrl2:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrl2:isRuleEnabled>
  <rdfs:comment>Produkt wird mit Individuum von Transform direkt verbunden (über isBeingProcessedWith)</rdfs:comment>
  <rdfs:label>Direkte Beziehung ableiten ProductFlow</rdfs:label>
</swrl:Imp>

<swrl:Imp>
  <swrl:body>
    <swrl:AtomList>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#AiSystem"/>
          <swrl:argument1 rdf:resource="#A"/>
