         xmlns:swrl2="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#">

<owl:Ontology rdf:about="http://www.semanticweb.org/testRegeln">
  <rdfs:comment>Folgende Regeln sind hier direkt implementiert:
-SWRL: Direkte Verbindungen (Assignment, Communication, Productflow)
-SWRL: Hinreichende Bedingung für CLoudFully und EdgeFully (Achtung Individuum von AI System nötig)</rdfs:comment>
</owl:Ontology>

<owl:ObjectProperty rdf:about="#communicatesWith">
  <rdfs:domain rdf:resource="#TechnicalResource"/>
  <rdfs:range rdf:resource="#TechnicalResource"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#composesOf">
  <owl:inverseOf rdf:resource="#isPartOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isPartOf">
  <rdfs:domain rdf:resource="#SystemElement"/>
  <rdfs:range rdf:resource="#AiSystem"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#flows">
  <owl:inverseOf rdf:resource="#isFlowing"/>
  <rdfs:domain rdf:resource="#Product"/>
  <rdfs:range rdf:resource="#ProductFlow"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isFlowing"/>

<owl:ObjectProperty rdf:about="#hasAssignment">
  <owl:inverseOf rdf:resource="#hasComponent"/>
  <rdfs:domain rdf:resource="#SystemComponent"/>
  <rdfs:range rdf:resource="#SystemFunction"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasComponent"/>

<owl:ObjectProperty rdf:about="#hasAssignmentComponent">
  <owl:inverseOf rdf:resource="#hasSystemComponentAssignment"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSystemComponentAssignment">
  <rdfs:domain rdf:resource="#SystemComponent"/>
  <rdfs:range rdf:resource="#Assignment"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasCommunication">
  <owl:inverseOf rdf:resource="#hasCommunicationComponent"/>
  <rdfs:domain rdf:resource="#SystemComponent"/>
  <rdfs:range rdf:resource="#Communication"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasCommunicationComponent"/>

<owl:ObjectProperty rdf:about="#hasFunction">
  <owl:inverseOf rdf:resource="#hasSystemFunctionAssignment"/>
  <rdfs:domain rdf:resource="#Assignment"/>
  <rdfs:range rdf:resource="#SystemFunction"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSystemFunctionAssignment"/>

<owl:ObjectProperty rdf:about="#isBeingProcessedWith">
  <owl:inverseOf rdf:resource="#processes"/>
  <rdfs:domain rdf:resource="#Product"/>
  <rdfs:range rdf:resource="#Transform"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#processes">
  <rdfs:domain rdf:resource="#Transform"/>
  <rdfs:range rdf:resource="#Product"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isTransformedBy">
  <owl:inverseOf rdf:resource="#isTransforming"/>
  <rdfs:domain rdf:resource="#ProductFlow"/>
  <rdfs:range rdf:resource="#Transform"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isTransforming"/>

<owl:DatatypeProperty rdf:about="#ErfuelltHinreichendeBedingungFuer">
  <rdfs:domain rdf:resource="#AiSystem"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>

<owl:Class rdf:about="#TechnicalResource">
  <rdfs:subClassOf rdf:resource="#SystemComponent"/>
</owl:Class>

<owl:Class rdf:about="#Product">
  <rdfs:subClassOf rdf:resource="#SystemComponent"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#flows"/>
      <owl:onClass rdf:resource="#ProductFlow"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <owl:disjointWith rdf:resource="#TechnicalResource"/>
</owl:Class>

<owl:Class rdf:about="#ProductFlow">
  <rdfs:subClassOf rdf:resource="#SystemRelation"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isFlowing"/>
      <owl:onClass rdf:resource="#Product"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isTransformedBy"/>
      <owl:onClass rdf:resource="#Transform"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#SystemComponent">
  <rdfs:subClassOf rdf:resource="#SystemElement"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCommunication"/>
      <owl:someValuesFrom rdf:resource="#Communication"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasSystemComponentAssignment"/>
      <owl:someValuesFrom rdf:resource="#Assignment"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#SystemFunction">
  <rdfs:subClassOf rdf:resource="#SystemElement"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasSystemFunctionAssignment"/>
      <owl:onClass rdf:resource="#Assignment"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Communication">
  <rdfs:subClassOf rdf:resource="#SystemRelation"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasCommunicationComponent"/>
      <owl:onClass rdf:resource="#SystemComponent"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Assignment">
  <rdfs:subClassOf rdf:resource="#SystemRelation"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasAssignmentComponent"/>
      <owl:onClass rdf:resource="#SystemComponent"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasFunction"/>
      <owl:onClass rdf:resource="#SystemFunction"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#Transform">
  <rdfs:subClassOf rdf:resource="#SystemFunction"/>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isTransforming"/>
      <owl:onClass rdf:resource="#ProductFlow"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#SystemElement">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isPartOf"/>
      <owl:someValuesFrom rdf:resource="#AiSystem"/>
    </owl:Restriction>
  </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#AiSystem">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#composesOf"/>
      <owl:onClass rdf:resource="#SystemElement"/>
      <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <owl:disjointWith rdf:resource="#SystemElement"/>
</owl:Class>

<owl:Class rdf:about="#Actuator">
  <rdfs:subClassOf rdf:resource="#TechnicalResource"/>
</owl:Class>

<owl:Class rdf:about="#SystemRelation">
  <rdfs:subClassOf rdf:resource="#SystemElement"/>
</owl:Class>

<owl:Class rdf:about="#Automate">
  <rdfs:subClassOf rdf:resource="#SystemFunction"/>
</owl:Class>

<owl:Class rdf:about="#CloudSystem">
  <rdfs:subClassOf rdf:resource="#TechnicalResource"/>
</owl:Class>

<owl:Class rdf:about="#ComputerSystem">
  <rdfs:subClassOf rdf:resource="#TechnicalResource"/>
</owl:Class>

<owl:Class rdf:about="#Controller">
  <rdfs:subClassOf rdf:resource="#TechnicalResource"/>
</owl:Class>

<owl:Class rdf:about="#EdgeDevice">
  <rdfs:subClassOf rdf:resource="#TechnicalResource"/>
</owl:Class>

<owl:Class rdf:about="#Inference">
  <rdfs:subClassOf rdf:resource="#SystemFunction"/>
</owl:Class>

<owl:Class rdf:about="#Process">
  <rdfs:subClassOf rdf:resource="#SystemFunction"/>
</owl:Class>

<owl:Class rdf:about="#Record">
  <rdfs:subClassOf rdf:resource="#SystemFunction"/>
</owl:Class>

<owl:Class rdf:about="#Sensor">
  <rdfs:subClassOf rdf:resource="#TechnicalResource"/>
</owl:Class>

<owl:Class rdf:about="#Store">
  <rdfs:subClassOf rdf:resource="#SystemFunction"/>
</owl:Class>

<owl:Class rdf:about="#Train">
  <rdfs:subClassOf rdf:resource="#SystemFunction"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#MyAiSystem">
  <rdf:type rdf:resource="#AiSystem"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#homoshit">
  <rdf:type rdf:resource="#Sensor"/>
</owl:NamedIndividual>

<swrl:Variable rdf:about="#A"/>

<swrl:Variable rdf:about="#CS"/>

<swrl:Variable rdf:about="#T"/>

<swrl:Variable rdf:about="#S"/>

<swrl:Variable rdf:about="#P1"/>

<swrl:Variable rdf:about="#P2"/>

<swrl:Variable rdf:about="#I"/>

<swrl:Variable rdf:about="#ED"/>

<swrl:Variable rdf:about="#B"/>

<swrl:Variable rdf:about="#C"/>

<swrl:Variable rdf:about="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#A"/>

<swrl:Variable rdf:about="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#B"/>

<swrl:Variable rdf:about="http://www.semanticweb.org/pauls/ontologies/2023/6/untitled-ontology-10#C"/>

<owl:AllDifferent>
  <owl:distinctMembers rdf:parseType="Collection">
    <rdf:Description rdf:about="#MyAiSystem"/>
    <rdf:Description rdf:about="#homoshit"/>
  </owl:distinctMembers>
</owl:AllDifferent>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Actuator"/>
    <rdf:Description rdf:about="#CloudSystem"/>
    <rdf:Description rdf:about="#ComputerSystem"/>
    <rdf:Description rdf:about="#Controller"/>
    <rdf:Description rdf:about="#EdgeDevice"/>
    <rdf:Description rdf:about="#Sensor"/>
  </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Assignment"/>
    <rdf:Description rdf:about="#Communication"/>
    <rdf:Description rdf:about="#ProductFlow"/>
  </owl:members>
</owl:AllDisjointClasses>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Automate"/>
    <rdf:Description rdf:about="#Inference"/>
    <rdf:Description rdf:about="#Process"/>
    <rdf:Description rdf:about="#Record"/>
    <rdf:Description rdf:about="#Store"/>
    <rdf:Description rdf:about="#Train"/>
